<%- include('partials/header'); %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div form-group>
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle">
    <label for="blogPost">Post</label>
    <textarea class="form-control" name="blogContent" cols="30" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include('partials/footer'); %>